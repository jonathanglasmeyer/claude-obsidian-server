# Claude Code OAuth Token (get from: claude setup-token)
CLAUDE_CODE_OAUTH_TOKEN=your-claude-oauth-token-here

# Obsidian Vault Path (absolute path to vault directory)
OBSIDIAN_VAULT_PATH=/srv/claude-jobs/obsidian-vault

# Redis Connection URL
REDIS_URL=redis://localhost:6379

# Server Port
PORT=3001

# Environment
NODE_ENV=development

# API Secret Key for endpoint protection (generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
API_SECRET_KEY=your_secret_key_here

# Claude User (for production deployment)
CLAUDE_USER=claude